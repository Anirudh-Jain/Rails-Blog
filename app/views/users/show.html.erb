<h1> User Profile</h1>
<h3 class="text-center mt-4"> First Name:<%= @user.first_name %></h3>
<h3 class="text-center mt-4"> Last Name:<%= @user.last_name %></h3>
<h3 class="text-center mt-4"> Age:<%= @user.last_name %></h3>
<h3 class="text-center mt-4"> Email:<%= @user.email %></h3>

<% if !not_logged_in? && @user == current_user %>
    <div class = 'text-center mt-4'><%= link_to 'Edit your profile', edit_user_path(@user), class: "btn btn-outline-info" %></div>
<% end %>

<h1>Articles</h1>

<h3 class="text-center mt-4">Articles</h3>

<%= render 'shared/article' %>